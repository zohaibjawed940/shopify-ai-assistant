{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors https://admin.shopify.com https://*.myshopify.com"
          }
        ]
      }
    ]
  }
  